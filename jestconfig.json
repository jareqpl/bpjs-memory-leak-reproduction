{
  "transform": {
    "^.+\\.(ts|tsx)$": "ts-jest"
  },
  "roots": ["<rootDir>"],
  "testMatch": null,
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts(x?)$",
  "testPathIgnorePatterns": ["/node_modules/"],
  "verbose": false,
  "testEnvironment": "jsdom",
  "setupFiles": ["jest-date-mock"],
  "reporters": ["default", "jest-junit"],
  "setupFilesAfterEnv": ["./jest/jest.setup.ts"],
  "preset": "ts-jest"
}
